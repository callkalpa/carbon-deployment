{{> header}}
{{> sub-header}}

<div class="row row-overide">

    <div class="col-sm-6 col-md-3">
        <div class="gadget-holder" data-gadget="request-info-box" data-type="error" id="bla-graph" style="height: 183px"></div>
    </div><!-- col-sm-6 -->

    <div class="col-sm-6 col-md-3">
        <div class="panel panel-two panel-stat">
            <div class="panel-heading">

                <div class="statistics">
                    <div class="row">
                        <div class="col-xs-8">
                            <small class="statistics-main">Response Time</small>
                            <i class="fa fa-clock-o fa-3x"></i>
                        </div>
                        <div class="col-xs-4">

                        </div>
                    </div><!-- row -->
                    <div class="arrow-box2"></div>
                    <div class="row">
                        <small class="measure-label">ms</small>

                        <div class="col-xs-4 panel-bottom-box left-radius box-two-color">
                            <h4>545</h4>
                            <small class="stat-label">Max</small>
                        </div>

                        <div class="col-xs-4 line-seperator panel-bottom-box box-two-color">
                            <h4>90</h4>
                            <small class="stat-label">Avg</small>
                        </div>

                        <div class="col-xs-4 line-seperator panel-bottom-box right-radius box-two-color">
                            <h4>10</h4>
                            <small class="stat-label">Min</small>
                        </div>
                    </div><!-- row -->
                </div><!-- statistics -->

            </div><!-- panel-heading -->
        </div><!-- panel -->
    </div><!-- col-sm-6 -->

    <div class="col-sm-6 col-md-3">
        <div class="panel panel-three panel-stat">
            <div class="panel-heading">

                <div class="statistics">
                    <div class="row">
                        <div class="col-xs-3">
                            <small class="statistics-main">Sessions</small>
                            <i class="fa fa-group fa-3x"></i>
                        </div>
                        <div class="col-xs-9">
                            <span> 23897</span>
                        </div>
                    </div><!-- row -->
                    <div class="arrow-box3"></div>
                    <div class="row">

                        <div class="col-xs-8 panel-bottom-box left-radius box-three-color">
                            <div class="h4-stat-label">Avg per min.</div>
                            <small class="stat-label">&nbsp;</small>
                        </div>

                        <div class="col-xs-4 panel-bottom-box  right-radius  box-three-color">
                            <h4>120</h4>
                            <small class="stat-label">&nbsp;</small>
                        </div>
                    </div><!-- row -->
                </div><!-- statistics -->

            </div><!-- panel-heading -->
        </div><!-- panel -->
    </div><!-- col-sm-6 -->

    <div class="col-sm-6 col-md-3">
        <div class="panel panel-four panel-stat">
            <div class="panel-heading">

                <div class="statistics">
                    <div class="row">
                        <div class="col-xs-3">
                            <small class="statistics-main">Errors</small>
                            <i class="fa fa-times-circle fa-3x"></i>

                        </div>
                        <div class="col-xs-9">
                            <span>123</span>
                        </div>
                    </div><!-- row -->
                    <div class="arrow-box4"></div>
                    <div class="row">

                        <div class="col-xs-12 panel-bottom-box  right-radius left-radius  box-four-color">
                            <h4>23%</h4>
                            <small class="stat-label">&nbsp;</small>
                        </div>
                    </div><!-- row -->
                </div><!-- statistics -->

            </div><!-- panel-heading -->
        </div><!-- panel -->
    </div><!-- col-sm-6 -->

</div>

<div class="row row-overide">

    {{>request-panel}}
    {{>response-panel}}
    {{>error-panel}}

</div>

<script language="javascript">
    $(document).ready(function() {
        $('#main_app_table').dataTable( {
            "ajax" :  '/dashboard/api/as-data.jag?action=apps'
        } );
    } );
</script>

