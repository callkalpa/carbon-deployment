{{> header}}
{{> sub-header}}

<div class="row row-overide">

    <div class="col-sm-6 col-md-3">
        <div class="gadget-holder" data-gadget="request-info-box" data-type="request-count" id="request-info-box" style="height: 183px"></div>
    </div><!-- col-sm-6 -->

    <div class="col-sm-6 col-md-3">
        <div class="gadget-holder" data-gadget="response-info-box" data-type="response-count" id="response-info-box" style="height: 183px"></div>
    </div><!-- col-sm-6 -->

    <div class="col-sm-6 col-md-3">
        <div class="gadget-holder" data-gadget="session-info-box" data-type="session-count" id="session-info-box" style="height: 183px"></div>
    </div><!-- col-sm-6 -->

    <div class="col-sm-6 col-md-3">
        <div class="gadget-holder" data-gadget="error-info-box" data-type="error-count" id="error-info-box" style="height: 183px"></div>
    </div><!-- col-sm-6 -->

</div>

<div class="row row-overide">

    {{>request-panel}}
    {{>response-panel}}
    {{>error-panel}}

</div>

<script language="javascript">
    $(document).ready(function() {
        $('#main_app_table').dataTable( {
            "ajax" :  '/dashboard/api/as-data.jag?action=apps'
        } );
    } );
</script>

