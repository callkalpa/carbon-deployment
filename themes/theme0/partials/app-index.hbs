{{> header}}
{{> sub-header}}

<div class="row row-overide">
    {{> info-boxes }}

</div>

<div class="row row-overide">

    <div class="col-sm-6">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="tabbable tabs-below">
                    <div class="tab-content">
                        <div class="tab-pane active" id="panel_one_tab1">
                            <div class="gadget-holder" data-gadget="pie-chart" data-type="browser" id="browser-graph" style="height: 300px"></div>
                        </div>
                        <div class="tab-pane " id="panel_one_tab2">
                            <div class="gadget-holder" data-gadget="pie-chart" data-type="operating-system" id="operating-system-graph" style="height: 300px"></div>
                        </div>
                        <div class="tab-pane" id="panel_one_tab3">
                            <div class="gadget-holder" data-gadget="pie-chart" data-type="device-type" id="device-type-graph" style="height: 300px"></div>
                        </div>
                    </div>
                </div>
            </div>
            <ul class="nav nav-tabs">
                <li><a href="#panel_one_tab1" data-toggle="tab">Browser</a></li>
                <li><a href="#panel_one_tab2" data-toggle="tab">OS</a></li>
                <li><a href="#panel_one_tab3" data-toggle="tab">Device Type</a></li>
            </ul>
        </div>

    </div>

    <div class="col-sm-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                Top 5 HTTP Response codes
            </div>
            <div class="panel-body">
                <div class="gadget-holder" data-gadget="bar-chart" data-type="top-http-status" id="top-http-status-graph" style="height: 300px"></div>
            </div>
        </div>

    </div>
</div>

<div class="row row-overide">
    <div class="col-sm-6">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="tabbable tabs-below">
                    <div class="tab-content">
                        <div class="tab-pane active" id="panel_two_tab1">
                            <div class="gadget-holder" data-gadget="location" data-type="country" id="location-map" style="height: 300px"></div>
                        </div>
                        <div class="tab-pane" id="panel_two_tab2">
                            <div class="gadget-holder" data-gadget="bar-chart" data-type="language" id="language-graph" style="height: 300px"></div>
                        </div>
                    </div>

                </div>
            </div>
            <ul class="nav nav-tabs">
                <li><a href="#panel_two_tab1" data-toggle="tab">Location</a></li>
                <li><a href="#panel_two_tab2" data-toggle="tab">Top 5 Languages</a></li>
            </ul>
        </div>

    </div>

    <div class="col-sm-6">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="tabbable tabs-below">
                    <div class="tab-content">
                        <div class="tab-pane active" id="one3_">
                            <div class="gadget-holder" data-gadget="data-table" data-type="context" id="context-table" style="height: 300px"></div>
                        </div>
                        <div class="tab-pane" id="two3_">
                            <div class="gadget-holder" data-gadget="data-table" data-type="referral" id="referral-table" style="height: 300px"></div>
                        </div>
                    </div>

                </div>
            </div>
            <ul class="nav nav-tabs">
                <li><a href="#one3_" data-toggle="tab">Context</a></li>
                <li><a href="#two3_" data-toggle="tab">Referrals</a></li>
            </ul>
        </div>

    </div>
</div>


