<serviceGroup>
    <service name="JNDIResourceConfigurationAdmin" scope="transportsession">
        <schema schemaNamespace="http://org.apache.axis2/xsd"
                elementFormDefaultQualified="true"/>
        <transports>
            <transport>https</transport>
        </transports>
        <description>
            JNDI Resource Manager Admin Service
        </description>
        <parameter name="ServiceClass">
            org.wso2.carbon.jndi.resource.mgt.services.JNDIResourceManager
        </parameter>
        <parameter name="adminService" locked="true">true</parameter>
        <parameter name="AuthorizationAction" locked="true">/permission/admin/manage</parameter>
        <parameter name="hiddenService" locked="true">true</parameter>
    </service>
</serviceGroup>